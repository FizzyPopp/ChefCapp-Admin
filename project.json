{
    "targets": [
        "adminClient",
        "ccApiEngine"
    ],
    "services": {
        "ccApiEngine": {
            "docker": {
                "repository": "chefcapp/ccapi"
            }
        }
    }
    "paths":{
        "adminClient": {
            "src": {
                "root": "/cc-admin-client/chef_capp_admin_client",
                "build": "/cc-admin-client/chef_capp_admin_client/build/web"
            },
            "dest":{
                "develop": "/public/admin-client/develop",
                "release": "/public/admin-client/release"
            }
        },
        "ccApiEngine": {
            "src": {
                "root": "/cc-api-engine",
                "package": "/cc-api-engine/build",
                "temp": "/cc-api-engine/temp"
            },
            "dest": {
                "temp": "/public/admin-server/tmp",
                "develop": "/public/admin-server/develop",
                "release": "/public/admin-server/release"
            }
        }
    }
}
